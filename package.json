{
  "name": "spot-graphql-server",
  "version": "0.3.0",
  "private": true,
  "engines": {
    "yarn": "1",
    "npm": "8",
    "node": "16"
  },
  "scripts": {
    "test": "npm run simpletest",
    "build": "sass public/stylesheets/style.sass:public/stylesheets/style.css",
    "simpletest": "ts-node test/schemaTest.ts",
    "printSchema": "ts-node bin/printSchema.ts",
    "watch": "npm run build ; ts-node-dev -r dotenv/config serverInit.ts",
    "start": "npm run build ; ts-node -r dotenv/config ./serverInit.ts",
    "prepare": "yarn run snyk-protect",
    "snyk-protect": "snyk-protect"
  },
  "dependencies": {
    "@envelop/core": "^2.3.1",
    "@graphql-tools/mock": "8.6.8",
    "@snyk/protect": "1.908.0",
    "body-parser": "1.20.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "debug": "4.3.4",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "express-graphql": "0.12.0",
    "graphql": "16.3.0",
    "graphql-helix": "^1.12.0",
    "morgan": "1.10.0",
    "node-fetch": "2.6.7",
    "sass": "1.50.1",
    "ts-node-dev": "1.1.8",
    "typescript": "4.6.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.27",
    "@types/node-fetch": "^2.6.1"
  },
  "snyk": true
}
