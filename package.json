{
  "name": "spot-graphql-server",
  "version": "0.3.0",
  "private": true,
  "engines": {
    "yarn": "1",
    "npm": "9",
    "node": "18.x"
  },
  "scripts": {
    "test": "npm run simpletest",
    "build": "sass public/stylesheets/style.sass:public/stylesheets/style.css",
    "simpletest": "ts-node test/schemaTest.ts",
    "printSchema": "ts-node bin/printSchema.ts",
    "watch": "npm run build ; ts-node-dev -r dotenv/config serverInit.ts",
    "start": "npm run build ; ts-node -r dotenv/config ./serverInit.ts",
    "prepare": "yarn run snyk-protect",
    "snyk-protect": "snyk-protect"
  },
  "dependencies": {
    "@envelop/core": "3.0.4",
    "@graphql-tools/mock": "8.7.14",
    "@snyk/protect": "1.1087.0",
    "cors": "2.8.5",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "graphql": "16.6.0",
    "graphql-helix": "1.13.0",
    "node-fetch": "3.3.0",
    "sass": "1.58.0",
    "ts-node-dev": "2.0.0",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@types/cors": "2.8.13",
    "@types/express": "4.17.17",
    "@types/node": "18.11.18",
    "@types/node-fetch": "2.6.2",
    "@vercel/node": "2.8.6"
  },
  "snyk": true
}
