{
  "name": "spot-graphql-server",
  "version": "0.3.0",
  "private": true,
  "engines": {
    "yarn": "1",
    "npm": "8",
    "node": "16.x"
  },
  "scripts": {
    "test": "npm run simpletest",
    "build": "sass public/stylesheets/style.sass:public/stylesheets/style.css",
    "simpletest": "ts-node test/schemaTest.ts",
    "printSchema": "ts-node bin/printSchema.ts",
    "watch": "npm run build ; ts-node-dev -r dotenv/config serverInit.ts",
    "start": "npm run build ; ts-node -r dotenv/config ./serverInit.ts",
    "prepare": "yarn run snyk-protect",
    "snyk-protect": "snyk-protect"
  },
  "dependencies": {
    "@envelop/core": "2.6.0",
    "@graphql-tools/mock": "8.7.6",
    "@snyk/protect": "1.989.0",
    "cors": "2.8.5",
    "dotenv": "16.0.2",
    "express": "4.18.1",
    "graphql": "16.6.0",
    "graphql-helix": "1.13.0",
    "node-fetch": "3.2.10",
    "sass": "1.54.9",
    "ts-node-dev": "2.0.0",
    "typescript": "4.8.3"
  },
  "devDependencies": {
    "@types/cors": "2.8.12",
    "@types/express": "4.17.14",
    "@types/node": "16.11.58",
    "@types/node-fetch": "2.6.2",
    "@vercel/node": "2.5.14"
  },
  "snyk": true
}
